{
  "$schema": "https://dl.viam.dev/module.schema.json",
  "module_id": "edss:edss-hx711",
  "visibility": "private",
  "url": "https://github.com/edouardss/edss-hx711",
  "description": "Modular sensor component: loadcell",
  "models": [
    {
      "api": "rdk:component:sensor",
      "model": "edss:hx711-loadcell:loadcell",
      "short_description": "basic loadcell sensor with a tare function, returning in kgs.",
      "markdown_link": "README_loadcell.md"
    },
    {
      "api": "rdk:component:sensor",
      "model": "edss:i2c-sensors:bmp-sensor",
      "short_description": "BMP altitude sensor for measuring atmospheric pressure and altitude.",
      "markdown_link": "README_bmp.md"
    }
  ],
  "entrypoint": "./run.sh",
  "first_run": "",
  "build": {
    "build": "./build.sh",
    "setup": "./setup.sh",
    "path": "dist/module.tar.gz",
    "arch": [
      "linux/amd64",
      "linux/arm64"
    ]
  }
}